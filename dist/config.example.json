{
    "baseURL": "https://rewards.bing.com/dashboard",
    "sessionPath": "sessions",
    "headless": false,
    "runOnZeroPoints": false,
    "clusters": 1,
    "errorDiagnostics": false,
    "workers": {
        "doDailySet": true,
        "doSpecialPromotions": true,
        "doMorePromotions": true,
        "doAppPromotions": true,
        "doDesktopSearch": true,
        "doMobileSearch": true,
        "doDailyCheckIn": true,
        "doReadToEarn": true,
        "doDailyStreak": true,
        "doRedeemGoal": true,
        "doDashboardInfo": true,
        "doClaimPoints": true
    },
    "searchOnBingLocalQueries": false,
    "globalTimeout": "30sec",
    "searchSettings": {
        "scrollRandomResults": false,
        "clickRandomResults": false,
        "parallelSearching": true,
        "queryEngines": ["google", "wikipedia", "reddit", "local"],
        "searchResultVisitTime": "10sec",
        "searchDelay": {
            "min": "30sec",
            "max": "1min"
        },
        "readDelay": {
            "min": "30sec",
            "max": "1min"
        }
    },
    "debugLogs": false,
    "consoleLogFilter": {
        "enabled": false,
        "mode": "whitelist",
        "levels": ["error", "warn"],
        "keywords": ["starting account"],
        "regexPatterns": []
    },
    "proxy": {
        "queryEngine": true
    },
    "webhook": {
        "discord": {
            "enabled": false,
            "url": ""
        },
        "ntfy": {
            "enabled": false,
            "url": "",
            "topic": "",
            "token": "",
            "title": "Microsoft-Rewards-Bot",
            "tags": ["bot", "notify"],
            "priority": 3
        },
        "webhookLogFilter": {
            "enabled": false,
            "mode": "whitelist",
            "levels": ["error"],
            "keywords": ["starting account", "select number", "collected"],
            "regexPatterns": []
        }
    },
    "redeemGoal": {
        "enabled": false,
        "skuUrl": "",
        "skuOptionValue": "",
        "redeemMode": "auto"
    }
}
